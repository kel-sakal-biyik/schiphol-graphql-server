{
  "name": "schiphol-graphql-api",
  "version": "1.0.0",
  "description": "An Express application to serve GraphQL interface for Schiphol API",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.ts --watch src/**/*.* --exec ts-node",
    "gql-gen": "gql-gen --url http://localhost:9999/graphql --template typescript --out ./src/typings/graphql-typings.d.ts ./src/**/*.graphql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kel-sakal-biyik/schiphol-graphql-api.git"
  },
  "keywords": [],
